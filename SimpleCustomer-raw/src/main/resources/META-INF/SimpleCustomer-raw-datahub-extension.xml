						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>addressID</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>street</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>street</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>streetnumber</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>streetnumber</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>postalcode</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>postalcode</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>town</name>
					<transformations>
						<transformation>
