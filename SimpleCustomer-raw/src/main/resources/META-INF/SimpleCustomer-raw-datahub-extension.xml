					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>country</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>region</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>region</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>phone1</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>phone1</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>objType</name>
					<transformations>
						<transformation>
							<rawSource>RawSimpleCustomer</rawSource>
							<expression>objType</expression>
						</transformation>
					</transformations>
				</attribute>
			</attributes>	
		</item>
	</canonicalItems>

</extension>
